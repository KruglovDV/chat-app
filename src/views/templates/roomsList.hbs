{{>header }}
<div class="container h-100">
    <div class="row h-100 justify-content-center align-items-center">
        <div class="col-10 col-md-8 col-lg-6">
            <h1 class="text-center">Rooms list</h1>
            <ul class="list-group">
                {{#each rooms}}
                    <li class="list-group-item"><a href="/room/{{this.name}}">{{this.name}}</a></li>
                {{/each}}
            </ul>
        </div>
    </div>
</div>